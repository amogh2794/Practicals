        <exception-type>java.lang.NumberFormatException</exception-type>
        <location>/ErrorServlet</location>
    </error-page>
    <servlet-mapping>
        <servlet-name>MyServletException</servlet-name>
        <url-pattern>/MyServletException</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processError</servlet-name>
        <url-pattern>/processError</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>500</error-code>
        <location>/AppExceptionHandler</location>
