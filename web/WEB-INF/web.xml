        <servlet-name>loginjdb</servlet-name>
        <url-pattern>/loginjdb</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resultstatus</servlet-name>
        <url-pattern>/resultstatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resdetails</servlet-name>
        <url-pattern>/resdetails</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
